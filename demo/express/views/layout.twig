<html>
    <head>
	{% block head %} {% endblock %}
	{% block meta %}
		<meta charset="utf-8">
		<meta http-equiv="content-type" content="text/html; charset=UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta name="generator" content="Nodefony (https://nodefony.net)">
		<meta name="keywords" content="web,Framework,realtime,node.js,symfony,javascript,npm,linux">
	{% endblock %}
	{% block description %}
		<meta name="description" content="Nodefony-stage framework Client">
	{% endblock %}
        <title>{% block title %}Nodefony-stage{% endblock %}</title>
        {% block stylesheets %}
		<link rel='stylesheet' href='/stylesheets/style.css' />
	{% endblock %}
    </head>
    <body>
	{% block body %}{% endblock %}
	{% block footer %}{% endblock %}
        {% block javascripts %}

		<script src="https://code.jquery.com/jquery-3.1.1.min.js"
			integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
			crossorigin="anonymous">
		</script>
		<script src="/javascripts/stage/stage6.js"></script>
	{% endblock %}
    </body>
</html>
